MODEL=first
IMPUTE_VALUE=-2
MODE=all
SCORE_PATH=./monomer_data_T0_EU/
SCORE_PATH_RAW=./monomer_data_T0_EU/raw_data/
SCORE_PATH_PROCESSED=./monomer_data_T0_EU/processed/
STAGE=0

rm -r $SCORE_PATH

echo "starting step0_data_process_EU.py"
python -u data_process.py --output_path $SCORE_PATH --phase $STAGE &
wait

echo  "starting step11_get_raw_score.py"
python -u step11_get_raw_score.py --model $MODEL --impute_value $IMPUTE_VALUE --mode $MODE --phase $STAGE --csv_path $SCORE_PATH_RAW  --out_path ./score_T0/ &
wait

